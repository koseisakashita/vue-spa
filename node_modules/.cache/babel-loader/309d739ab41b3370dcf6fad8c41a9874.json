eturn\n\n        this.$vuetify.application.bind(\n          this._uid,\n          this.applicationProperty,\n          this.updateApplication()\n        )\n      },\n      removeApplication (force = false) {\n        if (!force && !this.app) return\n\n        this.$vuetify.application.unbind(\n          this._uid,\n          this.applicationProperty\n        )\n      },\n      updateApplication: () => 0\n    }\n  })\n}\n"],"sourceRoot":""}]}