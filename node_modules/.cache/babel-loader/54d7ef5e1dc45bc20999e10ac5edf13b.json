y: Boolean\n  },\n\n  data: () => ({\n    isBooted: false\n  }),\n\n  computed: {\n    hasContent (): boolean | undefined {\n      return this.isBooted || !this.lazy || this.isActive\n    }\n  },\n\n  watch: {\n    isActive () {\n      this.isBooted = true\n    }\n  },\n\n  methods: {\n    showLazyContent (content?: VNode[]): VNode[] | undefined {\n      return this.hasContent ? content : undefined\n    }\n  }\n})\n"],"sourceRoot":""}]}