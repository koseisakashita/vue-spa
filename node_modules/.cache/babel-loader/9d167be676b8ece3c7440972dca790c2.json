P,IAAA,SAhBO,EAAA,SAAA,SAAA,GAgBE;AACP,aAAO,KAAA,KAAA,CAAA,GAAA,CAAP,aAAA;AACD;AAlBM,GAvC2E;AA4DpF,EAAA,MA5DoF,EAAA,SAAA,MAAA,CAAA,CAAA,EA4D3E;AACP,QAAM,OAAA,GAAqB;AACzB,MAAA,WAAA,EADyB,mBAAA;AAEzB,MAAA,KAAA,EAAO,KAFkB,MAAA;AAGzB,MAAA,KAAA,EAAO;AACL,QAAA,GAAA,EAAK,KADA,GAAA;AAEL,QAAA,GAAA,EAAK,KAAK;AAFL,OAHkB;AAOzB,MAAA,GAAA,EAAK;AAPoB,KAA3B;AAUA,QAAM,SAAA,GAAY,CAAA,CAAA,KAAA,EAAS;AACzB,MAAA,WAAA,EAAa;AADY,KAAT,EAEf,CACD,CAAA,CAAA,KAAA,EAHF,OAGE,CADC,CAFe,CAAlB;AAMA,QAAM,OAAA,GAAU,CAAA,CAAA,KAAA,EAAS;AACvB,MAAA,WAAA,EAAa;AADU,KAAT,EAEb,KAAA,MAAA,CAFH,OAAgB,CAAhB;AAIA,WAAO,CAAA,CAAA,KAAA,EAAS;AACd,MAAA,WAAA,EADc,YAAA;AAEd,MAAA,KAAA,EAAO;AACL,QAAA,MAAA,EAAW,KAAX,MAAW,GAAX;AADK,OAFO;AAKd,MAAA,EAAA,EAAI,KAAK;AALK,KAAT,EAMJ,CAAA,SAAA,EANH,OAMG,CANI,CAAP;AAOD;AAxFmF,CAAvE,CAAf","sourcesContent":["// Style\nimport '../../stylus/components/_parallax.styl'\n\n// Mixins\nimport Translatable from '../../mixins/translatable'\n\n// Types\nimport Vue from 'vue'\nimport { VNode, VNodeData } from 'vue/types/vnode'\nimport mixins, { ExtractVue } from '../../util/mixins'\n\ninterface options extends Vue {\n  $refs: {\n    img: HTMLImageElement\n  }\n}\n\n/* @vue/component */\nexport default mixins<options & ExtractVue<typeof Translatable>>(Translatable).extend({\n  name: 'v-parallax',\n\n  props: {\n    alt: {\n      type: String,\n      default: ''\n    },\n    height: {\n      type: [String, Number],\n      default: 500\n    },\n    src: String\n  },\n\n  data: () => ({\n    isBooted: false\n  }),\n\n  computed: {\n    styles (): object {\n      return {\n        display: 'block',\n        opacity: this.isBooted ? 1 : 0,\n        transform: `translate(-50%, ${this.parallax}px)`\n      }\n    }\n  },\n\n  watch: {\n    parallax () {\n      this.isBooted = true\n    }\n  },\n\n  mounted () {\n    this.init()\n  },\n\n  methods: {\n    init () {\n      const img = this.$refs.img\n\n      if (!img) return\n\n      if (img.complete) {\n        this.translate()\n        this.listeners()\n      } else {\n        img.addEventListener('load', () => {\n          this.translate()\n          this.listeners()\n        }, false)\n      }\n    },\n    objHeight () {\n      return this.$refs.img.naturalHeight\n    }\n  },\n\n  render (h): VNode {\n    const imgData: VNodeData = {\n      staticClass: 'v-parallax__image',\n      style: this.styles,\n      attrs: {\n        src: this.src,\n        alt: this.alt\n      },\n      ref: 'img'\n    }\n\n    const container = h('div', {\n      staticClass: 'v-parallax__image-container'\n    }, [\n      h('img', imgData)\n    ])\n\n    const content = h('div', {\n      staticClass: 'v-parallax__content'\n    }, this.$slots.default)\n\n    return h('div', {\n      staticClass: 'v-parallax',\n      style: {\n        height: `${this.height}px`\n      },\n      on: this.$listeners\n    }, [container, content])\n  }\n})\n"],"sourceRoot":""}]}